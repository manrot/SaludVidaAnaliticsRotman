        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido2)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NumeroLicenciaMedica)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumeroLicenciaMedica)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Identificacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Identificacion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdEspecialidadNavigation.NombreEspecialidad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEspecialidadNavigation.NombreEspecialidad)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdMedico" class="btn btn-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</div>
